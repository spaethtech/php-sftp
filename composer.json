{
    "name": "spaethtech/sftp",
    "description": "A Simple File Transfer Protocol (SFTP) wrapper.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-ssh2": "*"
    },
    "require-dev": {
