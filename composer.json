{
    "name": "rspaeth/sftp",
    "description": "A Simple File Transfer Protocol (SFTP) wrapper.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",
        "ext-ssh2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rspaeth\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
